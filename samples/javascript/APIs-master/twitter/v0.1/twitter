function url_replace(url, args, param, type, required, def) {
	if (args[param] === undefined && required && def !== undefined)
		args[param] = def;
	if (args[param] === undefined && required && def === undefined)
		throw new Error(param + ' is a required parameter');
	if (type == 'str' && args[param] !== undefined && args[param] !== undefined && args[param] !== null && typeof args[param] != 'string')
		args[param] = args[param].toString();
	if (type == 'int' && args[param] !== undefined && (parseFloat(args[param]) != parseInt(args[param], 10) || isNaN(args[param])))
		throw new Error(param + ' must be an integer');
	if (url.indexOf('{' + param + '}') !== -1) {
		var rgx = new RegExp('\\{' + param + '\\}', 'g');
		url = url.replace(rgx, args[param]);
		delete args[param];
	}
	return url;
}

var provider = fs('/bin/twitter');

exports.auth = function() { return auth({provider:provider}) };
/** 
 * Returns a collection of the most recent Tweets and retweets posted by the authenticating user and the users they follow. The home timeline is central to how most users interact with the Twitter service.               Up to 800 Tweets are obtainable on the home timeline. It is more volatile for users that follow many users or follow users who tweet frequently.
 * @category Timeline
 * @param {string} [format=json]  undefined
 * @param {string}  count   Specifies the number of records to retrieve. Must be less than or equal to 200. Defaults to 20.
 * @param {string}  max_id   Returns results with an ID less than (that is, older than) or equal to the specified ID.
 * @param {string}  since_id   Returns results with an ID greater than (that is, more recent than) the specified ID. There are limits to the number of Tweets which can be accessed through the API. If the limit of Tweets has occured since the since_id, the since_id will be forced to the oldest ID available.
 * @param {string}  trim_user   When set to either true, t or 1, each tweet returned in a timeline will include a user object including only the status authors numerical ID. Omit this parameter to receive the complete user object.
 * @param {boolean}  exclud_replies   This parameter will prevent replies from appearing in the returned timeline. Using exclude_replies with the count parameter will mean you will receive up-to count tweets — this is because the count parameter retrieves that many tweets before filtering out retweets and replies.
 * @param {boolean}  contributor_details   This parameter enhances the contributors element of the status response to include the screen_name of the contributor. By default only the user_id of the contributor is included.
 * @param {string}  include_entities   The entities node will be disincluded when set to false.
 * @methodhttp GET
 */
exports.homeTimeline = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/statuses/home_timeline.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'count', 'str');
	url = url_replace(url, args, 'max_id', 'str');
	url = url_replace(url, args, 'since_id', 'str');
	url = url_replace(url, args, 'trim_user', 'str');
	url = url_replace(url, args, 'exclud_replies');
	url = url_replace(url, args, 'contributor_details');
	url = url_replace(url, args, 'include_entities', 'str', false, "true");
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * API version 1.1               Updated on Mon, 2012-10-15 07:23               Returns a collection of the most recent Tweets posted by the user indicated by the screen_name or user_id parameters.               User timelines belonging to protected users may only be requested when the authenticated user either "owns" the timeline or is an approved follower of the owner.               The timeline returned is the equivalent of the one seen when you view a user's profile on twitter.com.               This method can only return up to 3,200 of a user's most recent Tweets. Native retweets of other statuses by the user is included in this total, regardless of whether include_rts is set to false when requesting this resource.
 * @category Timeline
 * @param {string} [format=json]  undefined
 * @param {integer}  count   Specifies the number of tweets to try and retrieve, up to a maximum of 200 per distinct request. The value of count is best thought of as a limit to the number of tweets to return because suspended or deleted content is removed after the count has been applied. We include retweets in the count, even if include_rts is not supplied. It is recommended you always send include_rts=1 when using this API method.
 * @param {integer}  user_id   The ID of the user for whom to return results for. Helpful for disambiguating when a valid user ID is also a valid screen name. Specifies the ID of the user to befriend. Helpful for disambiguating when a valid user ID is also a valid screen name.
 * @param {string}  screen_name   The screen name of the user for whom to return results for. Helpful for disambiguating when a valid screen name is also a user ID.
 * @param {integer}  since_id   Returns results with an ID greater than (that is, more recent than) the specified ID. There are limits to the number of Tweets which can be accessed through the API. If the limit of Tweets has occured since the since_id, the since_id will be forced to the oldest ID available.
 * @param {integer}  max_id   Returns results with an ID less than (that is, older than) or equal to the specified ID.
 * @param {string}  trim_user   When set to either true, t or 1, each tweet returned in a timeline will include a user object including only the status authors numerical ID. Omit this parameter to receive the complete user object.
 * @param {boolean}  exclude_replies   This parameter will prevent replies from appearing in the returned timeline. Using exclude_replies with the count parameter will mean you will receive up-to count tweets — this is because the count parameter retrieves that many tweets before filtering out retweets and replies. This parameter is only supported for JSON and XML responses.
 * @param {boolean}  contributor_details   This parameter enhances the contributors element of the status response to include the screen_name of the contributor. By default only the user_id of the contributor is included.
 * @param {boolean}  include_rts   When set to false, the timeline will strip any native retweets (though they will still count toward both the maximal length of the timeline and the slice selected by the count parameter). Note: If you're using the trim_user parameter in conjunction with include_rts, the retweets will still contain a full user object.
 * @methodhttp GET
 */
exports.userTimeline = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/statuses/user_timeline.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'count', 'int');
	url = url_replace(url, args, 'user_id', 'int');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'since_id', 'int');
	url = url_replace(url, args, 'max_id', 'int');
	url = url_replace(url, args, 'trim_user', 'str');
	url = url_replace(url, args, 'exclude_replies');
	url = url_replace(url, args, 'contributor_details');
	url = url_replace(url, args, 'include_rts');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns the 20 most recent mentions (tweets containing a users's @screen_name) for the authenticating user.               The timeline returned is the equivalent of the one seen when you view your mentions on twitter.com.               This method can only return up to 800 tweets
 * @category Timeline
 * @param {string} [format=json]  undefined
 * @param {integer}  count   Specifies the number of tweets to try and retrieve, up to a maximum of 200. The value of count is best thought of as a limit to the number of tweets to return because suspended or deleted content is removed after the count has been applied. We include retweets in the count, even if include_rts is not supplied. It is recommended you always send include_rts=1 when using this API method.
 * @param {integer}  max_id   Returns results with an ID less than (that is, older than) or equal to the specified ID.
 * @param {integer}  since_id   Returns results with an ID greater than (that is, more recent than) the specified ID. There are limits to the number of Tweets which can be accessed through the API. If the limit of Tweets has occured since the since_id, the since_id will be forced to the oldest ID available.
 * @param {boolean}  include_entities   The entities node will be disincluded when set to false.
 * @param {boolean}  contributor_details   This parameter enhances the contributors element of the status response to include the screen_name of the contributor. By default only the user_id of the contributor is included.
 * @methodhttp GET
 */
exports.mentionsTimeline = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/statuses/mentions_timeline.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'count', 'int');
	url = url_replace(url, args, 'max_id', 'int');
	url = url_replace(url, args, 'since_id', 'int');
	url = url_replace(url, args, 'include_entities');
	url = url_replace(url, args, 'contributor_details');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
