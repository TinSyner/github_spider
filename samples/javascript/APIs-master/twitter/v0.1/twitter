/** 
 * Updates the authenticating user's settings.
 * @category Account
 * @param {string} [format=json]  undefined
 * @param {string}  trend_location_woeid   The Yahoo! Where On Earth ID to use as the user's default trend location. Global information is available by using 1 as the WOEID. The woeid must be one of the locations returned by GET trends/available.Example Values: 1
 * @param {string}  sleep_time_enabled   When set to true, t or 1, will enable sleep time for the user. Sleep time is the time when push or SMS notifications should not be sent to the user.Example Values: true
 * @param {string}  start_sleep_time   The hour that sleep time should begin if it is enabled. The value for this parameter should be provided in ISO8601 format (i.e. 00-23). The time is considered to be in the same timezone as the user's time_zone setting.Example Values: 13
 * @param {string}  end_sleep_time   The hour that sleep time should end if it is enabled. The value for this parameter should be provided in ISO8601 format (i.e. 00-23). The time is considered to be in the same timezone as the user's time_zone setting.Example Values: 13
 * @param {string}  time_zone   The timezone dates and times should be displayed in for the user. The timezone must be one of the Rails TimeZone names.Example Values: Europe/Copenhagen, Pacific/Tongatapu
 * @param {string}  sleep_time_enabled   The language which Twitter should render in for this user. The language must be specified by the appropriate two letter ISO 639-1 representation. Currently supported languages are provided by GET help/languages.Example Values: it, en, es
 * @methodhttp POST
 */
exports.updateAccountSettings = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/account/settings.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'trend_location_woeid', 'str');
	url = url_replace(url, args, 'sleep_time_enabled', 'str');
	url = url_replace(url, args, 'start_sleep_time', 'str');
	url = url_replace(url, args, 'end_sleep_time', 'str');
	url = url_replace(url, args, 'time_zone', 'str');
	url = url_replace(url, args, 'sleep_time_enabled', 'str');
	return http.post({url:url, body:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns settings (including current trend, geo and sleep time information) for the authenticating user.
 * @category Account
 * @param {string} [format=json]  undefined
 * @methodhttp GET
 */
exports.getAccountSettings = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/account/settings.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Removes the uploaded profile banner for the authenticating user. Returns HTTP 200 upon success.
 * @category Account
 * @param {string} [format=json]  undefined
 * @methodhttp GET
 */
exports.removeProfileBanner = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/account/remove_profile_banner.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Uploads a profile banner on behalf of the authenticating user. For best results, upload an inferior 5MB image that is exactly 1252px by 626px. Images will be resized for a number of display options. Users with an uploaded profile banner will have a profile_banner_url node in their Users objects. More information about sizing variations can be found in User Profile Images and Banners and GET users/profile_banner.Profile banner images are processed asynchronously. The profile_banner_url and its variant sizes will not necessary be available directly after upload.
 * @category Account
 * @param {string} [format=json]  undefined
 * @param {string} [banner]  The Base64-encoded or raw image data being uploaded as the user's new profile banner.
 * @param {string}  width   The width of the preferred section of the image being uploaded in pixels. Use with height, offset_left, and offset_top to select the desired region of the image to use.                Note: If providing any one of the height, width, offset_left, or offset_top parameters, you must provide all of the sizing parameters.
 * @param {string}  height   The height of the preferred section of the image being uploaded in pixels. Use with width, offset_left, and offset_top to select the desired region of the image to use.                Note: If providing any one of the height, width, offset_left, or offset_top parameters, you must provide all of the sizing parameters.
 * @param {string}  offset_left   The number of pixels by which to offset the uploaded image from the left. Use with height, width, and offset_top to select the desired region of the image to use.                Note: If providing any one of the height, width, offset_left, or offset_top parameters, you must provide all of the sizing parameters.
 * @param {string}  offset_top   The number of pixels by which to offset the uploaded image from the top. Use with height, width, and offset_left to select the desired region of the image to use.                Note: If providing any one of the height, width, offset_left, or offset_top parameters, you must provide all of the sizing parameters.
 * @methodhttp GET
 */
exports.updateProfileBanner = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/account/update_profile_banner.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'banner', 'str', true);
	url = url_replace(url, args, 'width', 'str');
	url = url_replace(url, args, 'height', 'str');
	url = url_replace(url, args, 'offset_left', 'str');
	url = url_replace(url, args, 'offset_top', 'str');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns all lists the authenticating or specified user subscribes to, including their own. The user is specified using the user_id or screen_name parameters. If no user is given, the authenticating user is used.                       This method used to be GET lists in version 1.0 of the API and has been renamed for consistency with other call.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {integer}  user_id   The ID of the user for whom to return results for. Helpful for disambiguating when a valid user ID is also a valid screen name.
 * @param {string}  screen_name   The screen name of the user for whom to return results for. Helpful for disambiguating when a valid screen name is also a user ID.
 * @param {string}  reverse   Set this to true if you would like owned lists to be returned first. See description above for information on how this parameter works.Example Values: true
 * @methodhttp GET
 */
exports.getLists = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/list.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'user_id', 'int');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'reverse', 'str');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns the lists the specified user has been added to. If user_id or screen_name are not provided the memberships for the authenticating user are returned.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {integer}  user_id   The ID of the user for whom to return results for. Helpful for disambiguating when a valid user ID is also a valid screen name.
 * @param {string}  screen_name   The screen name of the user for whom to return results for. Helpful for disambiguating when a valid screen name is also a user ID.
 * @param {string}  filter_to_owned_lists   When set to true, t or 1, will return just lists the authenticating user owns, and the user represented by user_id or screen_name is a member of.       
 * @param {integer}  cursor   Breaks the results into pages. A single page contains 20 lists. Provide a value of -1 to begin paging. Provide values as returned in the response body's next_cursor and previous_cursor attributes to page back and forth in the list.
 * @methodhttp GET
 */
exports.listsMemberships = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/memberships.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'user_id', 'int');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'filter_to_owned_lists', 'str');
	url = url_replace(url, args, 'cursor', 'int');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

