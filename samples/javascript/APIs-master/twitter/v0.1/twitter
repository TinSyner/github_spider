	url = url_replace(url, args, 'cursor', 'int');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns the lists owned by the specified Twitter user. Private lists will only be shown if the authenticated user is also the owner of the lists.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {integer}  user_id   The ID of the user for whom to return results for. Helpful for disambiguating when a valid user ID is also a valid screen name.                    A user_id or screen_name must be provided.
 * @param {string}  screen_name   The screen name of the user for whom to return results for. Helpful for disambiguating when a valid screen name is also a user ID.                    A user_id or screen_name must be provided.
 * @param {string}  count   The amount of results to return per page. Defaults to 20. No more than 1000 results will ever be returned in a single page.
 * @param {integer}  cursor   Breaks the results into pages. A single page contains 20 lists. Provide a value of -1 to begin paging. Provide values as returned in the response body's next_cursor and previous_cursor attributes to page back and forth in the list. It is recommended to always use cursors when the method supports them.
 * @methodhttp GET
 */
exports.userListsOwnerships = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/ownerships.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'user_id', 'int');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'count', 'str');
	url = url_replace(url, args, 'cursor', 'int');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns the specified list. Private lists will only be shown if the authenticated user owns the specified list. Either a list_id or a slug is required. If providing a list_slug, an owner_screen_name or owner_id is also required.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {integer}  owner_id   The user ID of the user who owns the list being requested by a slug.
 * @param {string}  owner_screen_name   The screen name of the user who owns the list being requested by a slug.
 * @param {string}  slug   You can identify a list by its slug instead of its numerical id. If you decide to do so, note that you'll also have to specify the list owner using the owner_id or owner_screen_name parameters.
 * @param {integer}  list_id   The numerical id of the list.
 * @methodhttp GET
 */
exports.userListShow = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/show.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'owner_id', 'int');
	url = url_replace(url, args, 'owner_screen_name', 'str');
	url = url_replace(url, args, 'slug', 'str');
	url = url_replace(url, args, 'list_id', 'int');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Returns tweet timeline for members of the specified list. Retweets are included by default. You can use the include_rts=false parameter to omit retweet objects.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {integer}  list_id   The numerical id of the list.
 * @param {string}  slug   You can identify a list by its slug instead of its numerical id. If you decide to do so, note that you'll also have to specify the list owner using the owner_id or owner_screen_name parameters.
 * @param {string}  owner_id   The user ID of the user who owns the list being requested by a slug.
 * @param {string}  owner_screen_name   The screen name of the user who owns the list being requested by a slug.
 * @param {integer}  count   Specifies the number of results to retrieve per "page."
 * @param {integer}  max_id   Returns results with an ID less than (that is, older than) or equal to the specified ID.
 * @param {string}  since_id   Returns results with an ID greater than (that is, more recent than) the specified ID. There are limits to the number of Tweets which can be accessed through the API. If the limit of Tweets has occured since the since_id, the since_id will be forced to the oldest ID available.
 * @param {boolean}  include_entities   Entities are ON by default in API 1.1, each tweet includes a node called "entities". This node offers a variety of metadata about the tweet in a discreet structure, including: user_mentions, urls, and hashtags. You can omit entities from the result by using include_entities=false
 * @param {string}  include_rts   When set to either true, t or 1, the list timeline will contain native retweets (if they exist) in addition to the standard stream of tweets. The output format of retweeted tweets is identical to the representation you see in home_timeline.
 * @methodhttp GET
 */
exports.listStatuses = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/statuses.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'list_id', 'int');
	url = url_replace(url, args, 'slug', 'str');
	url = url_replace(url, args, 'owner_id', 'str');
	url = url_replace(url, args, 'owner_screen_name', 'str');
	url = url_replace(url, args, 'count', 'int');
	url = url_replace(url, args, 'max_id', 'int');
	url = url_replace(url, args, 'since_id', 'str');
	url = url_replace(url, args, 'include_entities');
	url = url_replace(url, args, 'include_rts', 'str');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Creates a new list for the authenticated user. Note that you can't create more than 20 lists per account.
 * @category List
 * @param {string} [format=json]  undefined
 * @param {string} [name]  The name for the list.                        A list's name must start with a letter and can consist only of 25 or fewer letters, numbers, "-", or "_" characters.                   
 * @param {string}  mode   Whether your list is public or private. Values can be public or private. If no mode is specified the list will be public.
 * @param {string}  description   The description to give the list.
 * @methodhttp POST
 */
exports.createList = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/create.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'name', 'str', true);
	url = url_replace(url, args, 'mode', 'str');
	url = url_replace(url, args, 'description', 'str');
	return http.post({url:url, body:args, auth:'oauth1', provider:provider}, opts);
};

