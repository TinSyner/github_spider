 * @param {integer}  user_id   The ID of the user for whom to return results for.
 * @param {string}  screen_name   The screen name of the user for whom to return results for.
 * @param {integer}  count   Specifies the number of records to retrieve. Must be less than or equal to 200. Defaults to 20.
 * @param {integer}  since_id   Returns results with an ID greater than (that is, more recent than) the specified ID. There are limits to the number of Tweets which can be accessed through the API. If the limit of Tweets has occured since the since_id, the since_id will be forced to the oldest ID available.
 * @param {integer}  max_id   Returns results with an ID less than (that is, older than) or equal to the specified ID.
 * @param {integer}  include_entities   The entities node will be omitted when set to false.
 * @param {string}  page   undefined
 * @methodhttp GET
 */
exports.favorites = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/favorites/list.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'user_id', 'int');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'count', 'int');
	url = url_replace(url, args, 'since_id', 'int');
	url = url_replace(url, args, 'max_id', 'int');
	url = url_replace(url, args, 'include_entities', 'int');
	url = url_replace(url, args, 'page', 'str');
	return http({url:url, params:args, auth:'oauth1', provider:provider}, opts);
};

