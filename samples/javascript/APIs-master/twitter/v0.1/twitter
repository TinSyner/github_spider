/** 
 * Adds multiple members to a list, by specifying a comma-separated list of member ids or screen names. The authenticated user must own the list to be able to add members to it. Note that lists can't have more than 500 members, and you are limited to adding up to 100 members to a list at a time with this method.                       Please note that there can be issues with lists that rapidly remove and add memberships. Take care when using these methods such that you are not too rapidly switching between removals and adds on the same list. Either a list_id or a slug is required. If providing a list_slug, an owner_screen_name or owner_id is also required.
 * @category List Members
 * @param {string} [format=json]  undefined
 * @param {integer}  list_id   The numerical id of the list.
 * @param {string}  slug   You can identify a list by its slug instead of its numerical id. If you decide to do so, note that you'll also have to specify the list owner using the owner_id or owner_screen_name parameters.
 * @param {string}  user_id   A comma separated list of user IDs, up to 100 are allowed in a single request.
 * @param {string}  screen_name   A comma separated list of screen names, up to 100 are allowed in a single request.
 * @param {string}  owner_screen_name   The screen name of the user who owns the list being requested by a slug.
 * @param {integer}  owner_id   The user ID of the user who owns the list being requested by a slug.
 * @methodhttp POST
 */
exports.listAddMembers = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/members/create_all.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'list_id', 'int');
	url = url_replace(url, args, 'slug', 'str');
	url = url_replace(url, args, 'user_id', 'str');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'owner_screen_name', 'str');
	url = url_replace(url, args, 'owner_id', 'int');
	return http.post({url:url, body:args, auth:'oauth1', provider:provider}, opts);
};

/** 
 * Removes the specified member from the list. The authenticated user must be the list's owner to remove members from the list. Either a list_id or a slug is required. If providing a list_slug, an owner_screen_name or owner_id is also required.
 * @category List Members
 * @param {string} [format=json]  undefined
 * @param {integer}  list_id   The numerical id of the list.
 * @param {string}  slug   You can identify a list by its slug instead of its numerical id. If you decide to do so, note that you'll also have to specify the list owner using the owner_id or owner_screen_name parameters.
 * @param {string}  user_id   The ID of the user to remove from the list. Helpful for disambiguating when a valid user ID is also a valid screen name.
 * @param {string}  screen_name   The screen name of the user for whom to remove from the list. Helpful for disambiguating when a valid screen name is also a user ID.
 * @param {string}  owner_screen_name   The screen name of the user who owns the list being requested by a slug.
 * @param {integer}  owner_id   The user ID of the user who owns the list being requested by a slug.
 * @methodhttp POST
 */
exports.listRemoveMember = function(args, opts) {
	var url = 'https://api.twitter.com/1.1/lists/members/destroy.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'list_id', 'int');
	url = url_replace(url, args, 'slug', 'str');
	url = url_replace(url, args, 'user_id', 'str');
	url = url_replace(url, args, 'screen_name', 'str');
	url = url_replace(url, args, 'owner_screen_name', 'str');
	url = url_replace(url, args, 'owner_id', 'int');
