{
  "displayName":"Sncf",
  "icon":"/images/api/explorer/sncf1.png",
  "base_url":"http://api.transilien.com",
  "default_auth":"manual",
  "auth":{
    "manual":{
      "request":{
        "httpauth":{
          "username":"{client_id}",
          "password":"{client_secret}"
        },
        "accept":"application/vnd.sncf.transilien.od.depart+xml",
        "format":"xml"
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        }
      ]
    }
  }
}