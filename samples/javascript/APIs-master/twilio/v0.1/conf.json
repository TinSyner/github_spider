{
  "displayName":"Twilio",
  "icon":"",
  "base_url":"https://api.twilio.com/2010-04-01/Accounts",
  "default_auth":"manual",
  "auth":{
    "manual":{
      "request":{
        "format":"json",
        "httpauth": {
          "username":"{AccountSid}",
          "password":"{AuthToken}"
        }
      },
      "parameters":[
        {
          "name":"AccountSid",
          "type":"string"
        },
        {
          "name":"AuthToken",
          "type":"string"
        }
      ]
    }
  }
}