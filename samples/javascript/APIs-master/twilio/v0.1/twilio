	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/OutgoingCallerIds/{OutgoingCallerIdSid}.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'OutgoingCallerIdSid', 'str', true);
	return http.delete({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Returns a list of local AvailablePhoneNumber resource representations, each representing a phone number that is					currently available for provisioning within your account.			
 * @category Available Phone Numbers
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account responsible for this caller Id
 * @param {string} IsoCountryCode  The ISO country code of this phone number
 * @param {string} [AreaCode]  Find phone numbers in the specified Area Code. Only available for North American numbers.
 * @param {string} [Contains]  A pattern to match phone numbers on. Valid characters are '*' and [0-9a-zA-Z]. The '*' character will match any single					digit.			
 * @param {string} [InRegion]  Limit results to a particular region (i.e. State/Province). Given a phone number, search within the same Region as that					number.			
 * @param {string} [InPostalCode]  Limit results to a particular postal code. Given a phone number, search within the same postal code as that number.			
 * @param {string} [NearLatLong]  Given a latitude/longitude pair lat,long find geographically close numbers within Distance miles.
 * @param {string} [NearNumber]  Given a phone number, find a geographically close number within Distance miles. Distance defaults to 25 miles.
 * @param {string} [InLata]  Limit results to a specific Local access and transport area (LATA). Given a phone number, search within the same LATA as					that number.			
 * @param {string} [InRateCenter]  Limit results to a specific rate center, or given a phone number search within the same rate center as that number.					Requires InLata to be set as well.			
 * @param {string} [Distance]  Specifies the search radius for a Near- query in miles. If not specified this defaults to 25 miles.
 * @methodhttp GET
 */
exports.getAvailableLocalPhoneNumberList = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/AvailablePhoneNumbers/{IsoCountryCode}/Local.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'IsoCountryCode', 'str', true);
	url = url_replace(url, args, 'AreaCode', 'str');
	url = url_replace(url, args, 'Contains', 'str');
	url = url_replace(url, args, 'InRegion', 'str');
	url = url_replace(url, args, 'InPostalCode', 'str');
	url = url_replace(url, args, 'NearLatLong', 'str');
	url = url_replace(url, args, 'NearNumber', 'str');
	url = url_replace(url, args, 'InLata', 'str');
	url = url_replace(url, args, 'InRateCenter', 'str');
	url = url_replace(url, args, 'Distance', 'str');
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Returns a list of toll-free AvailablePhoneNumber resource representations, each representing a phone number that is					currently available for provisioning within your account.			
