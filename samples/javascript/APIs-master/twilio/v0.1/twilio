/** 
 * Updates the Sandbox resource via POST.
 * @category Sandbox
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account responsible for this caller Id
 * @param {string} [VoiceUrl]  
 * @param {string} [VoiceMethod]  The HTTP method that should be used to request the above URL. Must be either GET or POST. Defaults to POST.
 * @param {string} [SmsUrl]  The URL that Twilio should request when somebody sends an SMS to the sandbox.
 * @param {string} [SmsMethod]  The HTTP method that should be used to request the SmsUrl. Must be either GET or POST. Defaults to POST.
 * @methodhttp POST
 */
exports.updateSandboxByPost = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/Sandbox.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'VoiceUrl', 'str');
	url = url_replace(url, args, 'VoiceMethod', 'str');
	url = url_replace(url, args, 'SmsUrl', 'str');
	url = url_replace(url, args, 'SmsMethod', 'str');
	return http.post({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Updates the Sandbox resource via PUT.
 * @category Sandbox
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account responsible for this caller Id
 * @param {string} [VoiceUrl]  
 * @param {string} [VoiceMethod]  The HTTP method that should be used to request the above URL. Must be either GET or POST. Defaults to POST.
 * @param {string} [SmsUrl]  The URL that Twilio should request when somebody sends an SMS to the sandbox.
 * @param {string} [SmsMethod]  The HTTP method that should be used to request the SmsUrl. Must be either GET or POST. Defaults to POST.
 * @methodhttp PUT
 */
exports.updateSandboxByPut = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/Sandbox.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'VoiceUrl', 'str');
	url = url_replace(url, args, 'VoiceMethod', 'str');
	url = url_replace(url, args, 'SmsUrl', 'str');
	url = url_replace(url, args, 'SmsMethod', 'str');
	return http.put({provider:provider,url:url,params:args, auth:'manual'}, opts);
};