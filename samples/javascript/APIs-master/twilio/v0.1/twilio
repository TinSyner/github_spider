/** 
 * Returns a notification list
 * @category Notifications
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account responsible for this caller Id
 * @param {string} [Log]  Only show notifications for this log, using the integer log values shown above.
 * @param {string} [MessageDate]  Only show notifications for this date. Should be formatted as YYYY-MM-DD. You can also specify inequality, such as					MessageDate(less than or equal to)YYYY-MM-DD for messages logged at or before midnight on a date, and					MessageDate(greater than or equal to )YYYY-MM-DD for messages logged at or after midnight on a date. When performing a					date range query with day-granularity make sure the right endpoint is midnight of the next day (e.g. to construct a					range query for all calls on YYYY-MM-DD use MessageDate(greater than or equal to)YYYY-MM-DD(and)MessageDate(less					than)YYYY-MM-(DD+1)).			
 * @methodhttp GET
 */
exports.getNotificationList = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/Notifications.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'Log', 'str');
	url = url_replace(url, args, 'MessageDate', 'str');
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

