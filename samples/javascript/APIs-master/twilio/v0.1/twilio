function url_replace(url, args, param, type, required, def) {
	if (args[param] === undefined && required && def !== undefined)
		args[param] = def;
	if (args[param] === undefined && required && def === undefined)
		throw new Error(param + ' is a required parameter');
	if (type == 'str' && args[param] !== undefined && args[param] !== undefined && args[param] !== null && typeof args[param] != 'string')
		args[param] = args[param].toString();
	if (url.indexOf('{' + param + '}') !== -1) {
		var rgx = new RegExp('\\{' + param + '\\}', 'g');
		url = url.replace(rgx, args[param]);
		delete args[param];
	}
	return url;
}

var provider = fs('/bin/twilio');

exports.auth = function() { return auth({provider:provider}) };
/** 
 * Returns the account details for the {AccountSid}.
 * @category Accounts
 * @param {string} format  The response format.			
 * @param {string} AccountSid  Represents the unique id of the Account
 * @methodhttp GET
 */
exports.getAccount = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Allows you to modify the 'FriendlyName' for the {AccountSid} via POST.
 * @category Accounts
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account
 * @param {string} [FriendlyName]  The human-readable description of an account.
 * @param {string} [Status]  Alter the status of this account: use closed to irreversibly close this account, suspended to temporarily suspend it, or active to reactivate it.
 * @methodhttp POST
 */
exports.updateAccountByPost = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'FriendlyName', 'str');
	url = url_replace(url, args, 'Status', 'str');
	return http.post({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Allows you to modify the 'FriendlyName' for the {AccountSid} via PUT.
 * @category Accounts
