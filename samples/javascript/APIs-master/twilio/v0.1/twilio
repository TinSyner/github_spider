	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'FriendlyName', 'str');
	url = url_replace(url, args, 'Status', 'str');
	return http.put({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Right now, your credentials are tied to one and only one account, so the list resource only contains your single					account.			
 * @category Accounts
 * @param {string} format  undefined
 * @param {string} [FriendlyName]  Only return the Account resources with friendly names that exactly match this name.
 * @param {string} [Status]  Only return Account resources with the given status. Can be closed, suspended or active.
 * @methodhttp GET
 */
exports.getAccountList = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'FriendlyName', 'str');
	url = url_replace(url, args, 'Status', 'str');
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Creates a new subaccount.			
 * @category Accounts
 * @param {string} format  undefined
 * @param {string} [FriendlyName]  A human readable description of the new subaccount, up to 64 characters. Defaults to "SubAccount Created at {YYYY-MM-DD HH:MM meridiam}".
 * @methodhttp POST
 */
exports.createSubAccount = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'FriendlyName', 'str');
	return http.post({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Send a new outgoing message.
 * @category SMS Messages
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account.
 * @param {string} From  A Twilio number enabled for SMS. Only phone numbers purchased from Twilio work here; you cannot (for example) spoof SMS					messages from your own cell phone number.			
 * @param {string} To  The number to call. Format with a '+' and country code e.g., +16175551212 (E.164 format). Twilio will also accept					unformatted US numbers e.g., (415) 555-1212, 415-555-1212.			
 * @param {string} Body  The text of the message you want to send, limited to 160 characters.
 * @param {string} [StatusCallback]  A URL that Twilio will POST to when your message is processed. Twilio will POST the SmsSid as well as SmsStatus=sent or					SmsStatus=failed.			
 * @methodhttp POST
 */
exports.sendSMS = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/SMS/Messages.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'From', 'str', true);
	url = url_replace(url, args, 'To', 'str', true);
	url = url_replace(url, args, 'Body', 'str', true);
	url = url_replace(url, args, 'StatusCallback', 'str');
	return http.post({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

/** 
 * Returns a list of SMS messages associated with your account identified by {AccountSid}.
 * @category SMS Messages
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account
 * @param {string} [From]  Only show SMS messages from this phone number
 * @param {string} [To]  Only show SMS messages to this phone number.
 * @param {string} [DateSent]  Only show SMS messages sent on this date, given as YYYY-MM-DD. Example: DateSent=2009-07-06. You can also specify					inequality, such as DateSent lt YYYY-MM-DD for SMS messages that were sent on or before midnight on a date, and DateSent					gt YYYY-MM-DD for SMS messages sent on or after midnight on a date.			
 * @methodhttp GET
 */
exports.getSMSList = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/SMS/Messages.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'From', 'str');
	url = url_replace(url, args, 'To', 'str');
	url = url_replace(url, args, 'DateSent', 'str');
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

