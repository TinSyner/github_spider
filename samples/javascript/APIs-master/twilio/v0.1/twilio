/** 
 * Returns a list of conferences within an account identified by {AccountSid}.
 * @category Conferences
 * @param {string} format  undefined
 * @param {string} AccountSid  Represents the unique id of the Account responsible for this caller Id
 * @param {string} [Status]  Only show conferences currently in with this status. May be init, in-progress, or completed.
 * @param {string} [FriendlyName]  List conferences who's FriendlyName is the exact match of this string.
 * @param {string} [DateCreated]  Only show conferences that started on this date, given as YYYY-MM-DD. You can also specify inequality, such as					DateCreated(less than or equal to)YYYY-MM-DD for conferences that started at or before midnight on a date, and					DateCreated(greater than or equal to)YYYY-MM-DD for conferences that started at or after midnight on a date.			
 * @param {string} [DateUpdated]  Only show conferences that were last updated on this date, given as YYYY-MM-DD. You can also specify inequality, such as					DateUpdated(less than or equal to)YYYY-MM-DD for conferences that were last updated at or before midnight on a date, and					DateUpdated(greater than or equal to)YYYY-MM-DD for conferences that were updated at or after midnight on a date.			
 * @methodhttp GET
 */
exports.getConferenceListForAccount = function(args, opts) {
	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/Conferences.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'Status', 'str');
	url = url_replace(url, args, 'FriendlyName', 'str');
	url = url_replace(url, args, 'DateCreated', 'str');
	url = url_replace(url, args, 'DateUpdated', 'str');
	return http({provider:provider,url:url,params:args, auth:'manual'}, opts);
};

