	var url = 'https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/IncomingPhoneNumbers.{format}';
	args = args || {};
	url = url_replace(url, args, 'format', 'str', true, "json");
	url = url_replace(url, args, 'AccountSid', 'str', true);
	url = url_replace(url, args, 'AreaCode', 'str', true);
	url = url_replace(url, args, 'FriendlyName', 'str');
	url = url_replace(url, args, 'VoiceUrl', 'str');
	url = url_replace(url, args, 'VoiceMethod', 'str');
	url = url_replace(url, args, 'VoiceFallbackUrl', 'str');
	url = url_replace(url, args, 'VoiceFallbackMethod', 'str');
	url = url_replace(url, args, 'StatusCallback', 'str');
	url = url_replace(url, args, 'StatusCallbackMethod', 'str');
	url = url_replace(url, args, 'SmsUrl', 'str');
	url = url_replace(url, args, 'SmsMethod', 'str');
	url = url_replace(url, args, 'SmsFallbackUrl', 'str');
	url = url_replace(url, args, 'SmsFallbackMethod', 'str');
