{ 
	"displayName":"Flickr",
	"icon":"",
	"base_url":"",
  "default_auth":"oauth1",
  "auth":{
    "none":{},
    "oauth1":{
      "request_token":{
        "url":"http://www.flickr.com/services/oauth/request_token",
        "params":{
          "oauth_consumer_key":"{client_id}",
          "oauth_callback":"{_callback_url}?{_callback_args}"
        }
      },
      "authorize":{
        "url":"http://www.flickr.com/services/oauth/authorize",
        "params":{
          "perms":"{permissions}"
        }
      },
      "access_token":{
        "url":"http://www.flickr.com/services/oauth/access_token",
        "params":{
          "oauth_consumer_key":"{client_id}"
        }
      },
      "request":{
        "format":"json",
	"get":{
		"api_key":"{client_id}",
    "format":"json",
    "nojsoncallback":"1"
	},
	"put":{
		"api_key":"{client_id}"
	},
	"post":{
		"api_key":"{client_id}"
	},
	"delete":{
		"api_key":"{client_id}"
	},
        "params":{
         "oauth_consumer_key":"{client_id}"
        }
      },
      "parameters":[
        {
          "name":"client_id",
          "type":"string"
        },
        {
          "name":"client_secret",
          "type":"string"
        },
        {
          "name":"permissions",
          "type":"string",
          "cardinality":"1",
          "values": {
            "read":"Read access to the api",
            "write":"Read and write access to the api",
            "delete":"Read, write and deletion access to the api"
          }
        }
      ]
    }
  }
}
